    "calcite-web": "github:Esri/calcite-web#v1.2.5",
    "cpx": "^1.5.0",
    "dojo-typings": "^1.11.9",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.3",
    "postcss-normalize-charset": "^1.1.0",
